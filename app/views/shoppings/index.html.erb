<div class="container">
  <div class="row">
    <div class="col-lg-12 col-md-6 ml-auto mr-auto">

      <h1>Shoppings</h1>
      <%= link_to 'New Shopping', new_shopping_path, class: 'btn btn-outline-primary' %>
      <table class="table table-striped table-hover">
        <thead>
        <tr>
          <th>#</th>
          <th>Name</th>
          <th></th>
          <th>Status</th>
          <th colspan="3"></th>
        </tr>
        </thead>
        <tbody id="shoppings">
        <%= render @shoppings %>
        </tbody>
      </table>



    </div>
  </div>
</div>

